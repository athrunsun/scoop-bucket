{
    "version": "2.32.0.windows.1",
    "homepage": "https://github.com/git-for-windows/git",
    "description": "Distributed version control system. China Taobao mirror.",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://npm.taobao.org/mirrors/git-for-windows/v2.32.0.windows.1/PortableGit-2.32.0-64-bit.7z.exe#/dl.7z",
            "hash": "cc561200eb89fdb19c75f901aea2740ffa57a627a31c1a6ef86a8d37e3186275"
        },
        "32bit": {
            "url": "https://npm.taobao.org/mirrors/git-for-windows/v2.32.0.windows.1/PortableGit-2.32.0-32-bit.7z.exe#/dl.7z",
            "hash": "29495a17a58ee5fb0345a18d25bf410bd2d1ead86896ba9d2eecf0b690d78f0a"
        }
    },
    "bin": [
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe",
        "git-bash.exe",
        "usr\\bin\\ssh.exe",
        "usr\\bin\\sshd.exe",
        "usr\\bin\\scp.exe",
        "usr\\bin\\sftp.exe",
        "usr\\bin\\ssh-add.exe",
        "usr\\bin\\ssh-agent.exe",
        "usr\\bin\\ssh-keygen.exe",
        "usr\\bin\\ssh-keyscan.exe",
        "usr\\bin\\tig.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "post_install": "git config --global credential.helper manager",
    "checkver": {
        "github": "https://github.com/git-for-windows/git",
        "regex": "v([\\w.]+)/PortableGit-(?<ver>[\\w.]+)-64-bit"
    },
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://npm.taobao.org/mirrors/git-for-windows/v$version/PortableGit-$matchVer-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://npm.taobao.org/mirrors/git-for-windows/v$version/PortableGit-$matchVer-32-bit.7z.exe#/dl.7z"
            }
        }
    }
}
