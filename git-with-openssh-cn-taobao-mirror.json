{
    "version": "2.33.0.windows.2",
    "homepage": "https://github.com/git-for-windows/git",
    "description": "Distributed version control system. China Taobao mirror.",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://npm.taobao.org/mirrors/git-for-windows/v2.33.0.windows.2/PortableGit-2.33.0.2-64-bit.7z.exe#/dl.7z",
            "hash": "bc030848e282d49e562ae2392a72501cf539322ad06ffe4cea8cf766f148dfe8"
        },
        "32bit": {
            "url": "https://npm.taobao.org/mirrors/git-for-windows/v2.33.0.windows.2/PortableGit-2.33.0.2-32-bit.7z.exe#/dl.7z",
            "hash": "ca65539faddd27a7036a4b9dcda3b1ecd4026f59258533d919c5cbe34727d583"
        }
    },
    "bin": [
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe",
        "git-bash.exe",
        "usr\\bin\\ssh.exe",
        "usr\\bin\\sshd.exe",
        "usr\\bin\\scp.exe",
        "usr\\bin\\sftp.exe",
        "usr\\bin\\ssh-add.exe",
        "usr\\bin\\ssh-agent.exe",
        "usr\\bin\\ssh-keygen.exe",
        "usr\\bin\\ssh-keyscan.exe",
        "usr\\bin\\tig.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "post_install": "git config --global credential.helper manager",
    "checkver": {
        "github": "https://github.com/git-for-windows/git",
        "regex": "v([\\w.]+)/PortableGit-(?<ver>[\\w.]+)-64-bit"
    },
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://npm.taobao.org/mirrors/git-for-windows/v$version/PortableGit-$matchVer-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://npm.taobao.org/mirrors/git-for-windows/v$version/PortableGit-$matchVer-32-bit.7z.exe#/dl.7z"
            }
        }
    }
}
