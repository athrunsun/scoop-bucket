{
    "version": "2.31.0.windows.1",
    "homepage": "https://github.com/git-for-windows/git",
    "description": "Distributed version control system. China Taobao mirror.",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://npm.taobao.org/mirrors/git-for-windows/v2.31.0.windows.1/PortableGit-2.31.0-64-bit.7z.exe#/dl.7z",
            "hash": "2ee5489f844362f6b079be7aa0811abdbe99b4538d66485ca996da2d144fb466"
        },
        "32bit": {
            "url": "https://npm.taobao.org/mirrors/git-for-windows/v2.31.0.windows.1/PortableGit-2.31.0-32-bit.7z.exe#/dl.7z",
            "hash": "24d594b284cef66b9fa6c6be17896e83381707870d7771b9bd5fc91c6c1568b8"
        }
    },
    "bin": [
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe",
        "git-bash.exe",
        "usr\\bin\\ssh.exe",
        "usr\\bin\\sshd.exe",
        "usr\\bin\\scp.exe",
        "usr\\bin\\sftp.exe",
        "usr\\bin\\ssh-add.exe",
        "usr\\bin\\ssh-agent.exe",
        "usr\\bin\\ssh-keygen.exe",
        "usr\\bin\\ssh-keyscan.exe",
        "usr\\bin\\tig.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "post_install": "git config --global credential.helper manager",
    "checkver": {
        "github": "https://github.com/git-for-windows/git",
        "regex": "v([\\w.]+)/PortableGit-(?<ver>[\\w.]+)-64-bit"
    },
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://npm.taobao.org/mirrors/git-for-windows/v$version/PortableGit-$matchVer-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://npm.taobao.org/mirrors/git-for-windows/v$version/PortableGit-$matchVer-32-bit.7z.exe#/dl.7z"
            }
        }
    }
}
