        "32bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%202.34.0/PortableGit-2.34.0-32-bit.7z.exe#/dl.7z",
            "hash": "46643dde0f40bee6e7963b9d67ec2f78ffc6882d452b866940e5fe5724dbdba3"
        }
    },
    "bin": [
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe",
        "git-bash.exe",
        "usr\\bin\\ssh.exe",
        "usr\\bin\\sshd.exe",
        "usr\\bin\\scp.exe",
        "usr\\bin\\sftp.exe",
        "usr\\bin\\ssh-add.exe",
        "usr\\bin\\ssh-agent.exe",
        "usr\\bin\\ssh-keygen.exe",
        "usr\\bin\\ssh-keyscan.exe",
        "usr\\bin\\tig.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "post_install": "git config --global credential.helper manager",
    "checkver": {
        "github": "https://github.com/git-for-windows/git",
        "regex": "v[\\w.]+/PortableGit-([\\w.]+)-64-bit"
    },
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%20$version/PortableGit-$version-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
