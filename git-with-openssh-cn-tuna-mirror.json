{
    "version": "2.35.0",
    "homepage": "https://github.com/git-for-windows/git",
    "description": "Distributed version control system. China TUNA mirror.",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%202.35.0/PortableGit-2.35.0-64-bit.7z.exe#/dl.7z",
            "hash": "5a7e229acec0dfe840055a1327156fe1fdf0ed4ad709e12fab4f1a0e3dca39c3"
        },
        "32bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%202.35.0/PortableGit-2.35.0-32-bit.7z.exe#/dl.7z",
            "hash": "e291afdcfd05aa81e5d0b6928e3ca28094239e8f51e3fba26feb5a61a0c173d4"
        }
    },
    "bin": [
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe",
        "git-bash.exe",
        "usr\\bin\\ssh.exe",
        "usr\\bin\\sshd.exe",
        "usr\\bin\\scp.exe",
        "usr\\bin\\sftp.exe",
        "usr\\bin\\ssh-add.exe",
        "usr\\bin\\ssh-agent.exe",
        "usr\\bin\\ssh-keygen.exe",
        "usr\\bin\\ssh-keyscan.exe",
        "usr\\bin\\tig.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "post_install": "git config --global credential.helper manager",
    "checkver": {
        "github": "https://github.com/git-for-windows/git",
        "regex": "v[\\w.]+/PortableGit-([\\w.]+)-64-bit"
    },
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%20$version/PortableGit-$version-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%20$version/PortableGit-$version-32-bit.7z.exe#/dl.7z"
            }
        }
    }
}
