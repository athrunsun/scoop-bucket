{
    "version": "2.33.0.windows.1",
    "homepage": "https://github.com/git-for-windows/git",
    "description": "Distributed version control system. China Taobao mirror.",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://npm.taobao.org/mirrors/git-for-windows/v2.33.0.windows.1/PortableGit-2.33.0-64-bit.7z.exe#/dl.7z",
            "hash": "12c10fad2c2db17d9867dbbacff1adc8be50868b793a73d451c2b878914bb32d"
        },
        "32bit": {
            "url": "https://npm.taobao.org/mirrors/git-for-windows/v2.33.0.windows.1/PortableGit-2.33.0-32-bit.7z.exe#/dl.7z",
            "hash": "c3b6f1a8f8c1b5be2175b7190d35926dce07a58294780291326a437ef0694676"
        }
    },
    "bin": [
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe",
        "git-bash.exe",
        "usr\\bin\\ssh.exe",
        "usr\\bin\\sshd.exe",
        "usr\\bin\\scp.exe",
        "usr\\bin\\sftp.exe",
        "usr\\bin\\ssh-add.exe",
        "usr\\bin\\ssh-agent.exe",
        "usr\\bin\\ssh-keygen.exe",
        "usr\\bin\\ssh-keyscan.exe",
        "usr\\bin\\tig.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "post_install": "git config --global credential.helper manager",
    "checkver": {
        "github": "https://github.com/git-for-windows/git",
        "regex": "v([\\w.]+)/PortableGit-(?<ver>[\\w.]+)-64-bit"
    },
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://npm.taobao.org/mirrors/git-for-windows/v$version/PortableGit-$matchVer-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://npm.taobao.org/mirrors/git-for-windows/v$version/PortableGit-$matchVer-32-bit.7z.exe#/dl.7z"
            }
        }
    }
}
