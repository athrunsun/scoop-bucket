{
    "version": "2.31.1",
    "homepage": "https://github.com/git-for-windows/git",
    "description": "Distributed version control system. China TUNA mirror.",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%202.31.1/PortableGit-2.31.1-64-bit.7z.exe#/dl.7z",
            "hash": "fce2161a8891c4deefdb8d215ab76498c245072f269843ef1a489c4312baef52"
        },
        "32bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%202.31.1/PortableGit-2.31.1-32-bit.7z.exe#/dl.7z",
            "hash": "d6d48e16e3f0ecbc0a45d410ad3ebae15e5618202855ebe72cd9757e4d35b880"
        }
    },
    "bin": [
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe",
        "git-bash.exe",
        "usr\\bin\\ssh.exe",
        "usr\\bin\\sshd.exe",
        "usr\\bin\\scp.exe",
        "usr\\bin\\sftp.exe",
        "usr\\bin\\ssh-add.exe",
        "usr\\bin\\ssh-agent.exe",
        "usr\\bin\\ssh-keygen.exe",
        "usr\\bin\\ssh-keyscan.exe",
        "usr\\bin\\tig.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "post_install": "git config --global credential.helper manager",
    "checkver": {
        "github": "https://github.com/git-for-windows/git",
        "regex": "v[\\w.]+/PortableGit-([\\w.]+)-64-bit"
    },
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%20$version/PortableGit-$version-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%20$version/PortableGit-$version-32-bit.7z.exe#/dl.7z"
            }
        }
    }
}
